<?xml version="1.0" encoding="UTF-8"?>
<Configuration>         

    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %c{1}(%L) %M  - %msg%n"/>
        </Console>

    </Appenders>
    <Loggers>
        <Root  level="Debug">
            <AppenderRef ref="Console"  level="TRACE"/>
        </Root>
    </Loggers>
</Configuration>